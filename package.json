{
  "devDependencies": {
    "concurrently": "^8.2.0",
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3",
    "electron-is-dev": "^2.0.0",
    "wait-on": "^7.0.1"
  },
  "name": "my-finance-tracker",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run start-frontend\"  \"npm run start-backend\" \"npm run start-electron\" ",
    "start-backend": "cd backend && py app.py",
    "start-frontend": "cd finance-tracker-app && npm start",
    "build": "cd finance-tracker-app && npm run build",
    "start-electron": "electron .",
    "electron-pack": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
